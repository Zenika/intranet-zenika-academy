  "scripts": {
    "start": "node ./src/app.js",
    "dev": "nodemon --watch ./src --exec node --require dotenv/config ./src/app.js",
    "pm2:start": "pm2 start ./src/app.js --watch",
    "pm2:kill": "pm2 kill",
    "pm2:log": "pm2 logs",
    "test": "jest --forceExit",
    "migrate": "sequelize-cli db:migrate",
    "lint": "eslint ./src/**",
    "prettier": "prettier \"src/{**/,}*.{js,json}\" \"test/{**/,}*.{js,json}\"",
    "prettier:check": "npm run prettier -- --check",
    "prettier:write": "npm run prettier -- --write",
    "sql": "dotenv sh sql.sh"
  },
  "dependencies": {
    "@hapi/joi": "^16.1.7",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.19.0",
    "cookie-parser": "^1.4.5",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "jsonwebtoken": "^8.5.1",
    "morgan": "^1.9.1",
    "mysql2": "^2.0.0",
    "pug": "^2.0.4",
    "sequelize": "^5.21.2"
  },
  "devDependencies": {
    "dotenv": "^8.2.0",
    "dotenv-cli": "^3.1.0",
    "eslint": "^6.6.0",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-plugin-import": "^2.18.2",
    "jest": "^24.9.0",
    "nodemon": "^2.0.2",
    "pm2": "^4.1.2",
    "prettier": "^1.19.1",
    "sequelize-cli": "^5.5.1",
    "sqlite3": "^4.1.1",
    "supertest": "^4.0.2"
